<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sys="clr-namespace:System;assembly=netstandard">
  <Styles.Resources>
    
    <Color x:Key="MainDeepColor">#673AB7</Color>
    <Color x:Key="DeepAccentColor">#66673AB7</Color>
    <Color x:Key="DeepAccentColor1">#AA673AB7</Color>
    <Color x:Key="DeepAccentColor2">#BB673AB7</Color>
    <Color x:Key="LightGrayColor">#66404040</Color>
    <Color x:Key="LightAccentColor">#33673AB7</Color>

    <SolidColorBrush x:Key="MainDeepBrush" Color="{DynamicResource MainDeepColor}" />
    <SolidColorBrush x:Key="DeepAccentBrush" Color="{DynamicResource DeepAccentColor}" />
    <SolidColorBrush x:Key="DeepAccentBrush1" Color="{DynamicResource DeepAccentColor1}" />
    <SolidColorBrush x:Key="DeepAccentBrush2" Color="{DynamicResource DeepAccentColor2}" />
    <SolidColorBrush x:Key="LightAccentBrush" Color="{DynamicResource LightAccentColor}" />
    <SolidColorBrush x:Key="LightGrayBrush" Color="{DynamicResource LightGrayColor}" />
  
  </Styles.Resources>
</Styles>